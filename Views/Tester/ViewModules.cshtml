@model IEnumerable<ReleaseManagementMVC.Models.Module>

@{
    ViewBag.Title = "ViewModules";
}

<h2>ViewModules</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModuleID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProjectID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeveloperID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TesterID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.ModuleID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ModuleName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ProjectID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ModuleStatus)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DeveloperID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TesterID)
    </td>
</tr>
}

</table>
